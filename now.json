{
  "version": 2,
  "name": "linus-blog",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    },
    {
      "src": "api/index.js",
      "use": "@next/node"
    }
  ],
  "routes": [
    {
      "src": "/graphql(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "next.config.js"
    }
  ]
}
